<div
  class="d-flex flex-column flex-shrink-0 mt-2 p-3 text-black box"
  style="width: 430px; height: fit-content"
>
  <h4 style="text-align: center">Status</h4>
  <hr style="margin-top: 0; margin-bottom: 7%" />
  <div>
    <div class="row">
      <div class="col">
        <h5>HMC-Magnetometer</h5>
      </div>
      <div class="col">
        <div class="form-check form-switch" style="padding-left: 90%">
          <input class="form-check-input" type="checkbox" id="switchImu" />
        </div>
      </div>
    </div>
    <p style="margin-top: 4%">Yaw: <span id="imuData">0</span></p>
  </div>
  <hr style="margin-bottom: 7%" />
  <div>
    <div class="row">
      <div class="col">
        <h5>GPS</h5>
      </div>
      <div class="col">
        <div class="form-check form-switch" style="padding-left: 90%">
          <input class="form-check-input" type="checkbox" id="switchGPS" />
        </div>
      </div>
    </div>
    <p style="margin-top: 4%; margin-bottom: 2%">
      Lat: <span id="gpsLat">0</span>
    </p>
    <p style="margin-top: 0; margin-bottom: 2%">
      Long: <span id="gpsLong">0</span>
    </p>
    <p style="margin-bottom: 2%">
      Distance: <span id="goalDistance">0.0</span>
    </p>
  </div>
  <hr style="margin-bottom: 7%" />
  <div>
    <div class="row">
      <div class="col">
        <!-- <h5>Speed</h5> -->
        <h5>Vector</h5>
      </div>
      <div class="col">
        <div class="form-check form-switch" style="padding-left: 90%">
          <input class="form-check-input" type="checkbox" id="switchSpeed" />
        </div>
      </div>
    </div>
    <p style="margin-top: 4%; margin-bottom: 2%">
      Goal_Index , Final_index: <span id="cmdVelV">0.0 , 0.0</span>
    </p>
  </div>
  <hr style="margin-bottom: 7%" />
